<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="question">
    <strong>@ViewBag.question</strong>

    <form class="options" action="/Home/Questions" method="post">

        @foreach (var option in ViewBag.options)
        {
            <input name="button" type="submit" value="@option" />

        }
    </form>

</div>